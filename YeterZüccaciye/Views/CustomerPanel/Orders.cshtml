@using YeterZüccaciye.Models.Sınıflar
@model List<SalesMove>
@{
    ViewBag.Title = "Orders";
    Layout = "~/Views/Shared/_CustomerLayout.cshtml";
}
<br />
<h2>Sipariş Geçmişim</h2>
<br />
<table class="table table-bordered">
    <tr>
        <th>ÜRÜN AD</th>
        <th>FİYAT</th>
        <th>ADET</th>
        <th>TARİH</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.Product.PRODUCTNAME</td>
            <td>@x.Price</td>
            <td>@x.Amount</td>
            <td>@Convert.ToDateTime(x.SalesDate).ToString("dd/MM/yy")</td>
        </tr>
    }

</table>
